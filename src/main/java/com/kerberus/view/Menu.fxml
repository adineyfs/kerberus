<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>

<Pane fx:id="menuContainer" styleClass="menuContainer" stylesheets="@Menu.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
       <VBox styleClass="menuLogoContainer">
           <Label fx:id="menuLabel" styleClass="labelTecsys" text="TECSYS" />
           <ImageView fx:id="menuLogo" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" styleClass="menuLogo">
              <image>
                 <Image url="Kerberus_logo.png" />
              </image>
              <effect>
                 <ColorAdjust brightness="0.40" contrast="0.02" hue="1.0" saturation="-1.0" />
              </effect>
           </ImageView>
           <Label fx:id="menuLabel" styleClass="labelKerberus" text="Kerberus" />
       </VBox>
    </children>
</Pane>
