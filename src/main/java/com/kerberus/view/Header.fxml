<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.geometry.*?>

<HBox fx:id="mainHeader" onMouseDragged="#handleOnMouseDragged" onMousePressed="#handleOnMousePressed" styleClass="mainHeader" stylesheets="@Header.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kerberus.controller.HeaderController">
   <children>
      <Button fx:id="btnMinimize" mnemonicParsing="false" onAction="#handleBtnMinimize" styleClass="btnMinimize" />
      <Button fx:id="btnMaximize" mnemonicParsing="false" onAction="#handleBtnMaximize" styleClass="btnMaximize" />
      <Button fx:id="btnClose" mnemonicParsing="false" onAction="#handleBtnClose" styleClass="btnClose" />
   </children>
</HBox>
